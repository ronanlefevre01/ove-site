{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://opti-admin.vercel.app/api/$1"
    },
    {
      "source": "/(assets/.*)",
      "destination": "/$1"
    },
    {
      "source": "/(favicon\\.ico|robots\\.txt|manifest\\.json)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
